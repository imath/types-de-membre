{
	"name": "imath/types-de-membre",
	"type": "buddypress-plugin",
	"description": "Adds a dropdown control to filter the BuddyPress Members directory by types",
	"keywords": [
		"BuddyPress",
		"Members",
		"Type",
		"Dropdown"
	],
	"homepage": "https://github.com/imath/types-de-membre",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Contributors",
			"homepage": "https://github.com/imath/types-de-membre/graphs/contributors"
		}
	],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"squizlabs/php_codesniffer" : "^3.5.4",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
		"phpcompatibility/phpcompatibility-wp" : "*",
		"wp-coding-standards/wpcs": "*",
		"php-parallel-lint/php-parallel-lint": "^1.2.0",
		"wp-cli/i18n-command": "^2.2.5"
	},
	"scripts": {
		"lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
		"lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
		"make-pot": "wp i18n make-pot . languages/types-de-membre.pot"
	},
	"support": {
		"issues": "https://github.com/imath/types-de-membre/issues",
		"source": "https://github.com/imath/types-de-membre"
	}
}
